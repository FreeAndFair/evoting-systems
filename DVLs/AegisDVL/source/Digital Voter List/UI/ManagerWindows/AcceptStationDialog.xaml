<Window x:Class="UI.ManagerWindows.AcceptStationDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Accept Station" Height="300" Width="300"
        WindowStartupLocation="CenterScreen"
        Icon="../Icon.ico">
    <Grid>
        <Label Content="Indtast kodeord (vist på stationens skærm)" Height="28" HorizontalAlignment="Left" Margin="20,12,0,0" Name="TitleLabel" VerticalAlignment="Top" />
        <PasswordBox Height="45" HorizontalAlignment="Left" Margin="38,83,0,0" Name="PwTextbox" VerticalAlignment="Top" Width="206" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" PasswordChanged="PwTextboxPasswordChanged" />
        <Button Content="Annuller" Height="23" HorizontalAlignment="Left" Margin="49,184,0,0" Name="CancelButton" VerticalAlignment="Top" Width="75" Click="CancelButtonClick" IsCancel="True"/>
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="156,184,0,0" Name="OKButton" VerticalAlignment="Top" Width="75" Click="OkButtonClick" IsDefault="True" />
        <Label Content="Station Navn" Height="28" HorizontalAlignment="Center" Margin="36,49,34,0" Name="StationLabel" VerticalAlignment="Top" Width="208" HorizontalContentAlignment="Center" />
    </Grid>
</Window>
